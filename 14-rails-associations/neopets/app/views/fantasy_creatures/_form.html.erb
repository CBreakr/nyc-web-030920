<%= form_for @fantasy_creature do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :strength %>
    <%= f.number_field :strength %>
    <br>
    <%= f.label :environment %>
    <%= f.text_field :environment %>
    <br>
    <%= f.label :finishing_move %>
    <%= f.text_field :finishing_move %>
    <br>
    <%= f.label :img_url %>
    <%= f.text_area :img_url %>
    <br>
    <%= f.submit %>
<% end %>